{
  "name": "stacks-token-streaming-tests",
  "version": "1.0.0",
  "description": "Run unit tests on this project.",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --config frontend/vite.config.ts",
    "build": "vite build --config frontend/vite.config.ts",
    "preview": "vite preview --config frontend/vite.config.ts",
    "test": "vitest run",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "chokidar \"tests/**/*.ts\" \"contracts/**/*.clar\" -c \"npm run test:report\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hirosystems/clarinet-sdk": "^2.3.2",
    "@stacks/connect": "^7.5.0",
    "@stacks/connect-react": "^23.1.0",
    "@stacks/network": "^6.9.0",
    "@stacks/transactions": "^6.12.0",
    "chokidar-cli": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.170.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vitest-environment-clarinet": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.14",
    "@types/three": "^0.170.0",
    "@vitejs/plugin-react": "^4.3.4"
  }
}